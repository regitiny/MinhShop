/**
 * The PostDetails entity.
 * @author A true hipster
 */
entity PostDetails {
    /** uuid */
    uuid UUID required unique
    /** postDetailsId */
    postDetailsId String required unique minlength(3) maxlength(16) pattern(/[A-z]+[0-9]+/ )
    /** content */
    content TextBlob

    /** searchField */
    searchField TextBlob
    /** role */
    role String maxlength(DEFAULT_MAX_LENGTH)
    /** createdDate */
    createdDate  Instant
    /** modifiedDate */
    modifiedDate  Instant
    /** createdBy */
    createdBy String
    /** modifiedBy */
    modifiedBy String
    /** dataSize */
    dataSize Long
    /** comment */
    comment String maxlength(DEFAULT_MAX_LENGTH)
}

